{
	"extends": "react-app",
	"plugins": [
		"react-hooks"
	],
	"rules": {
		"comma-dangle": 0,
		"no-underscore-dangle": 0,
		"no-param-reassign": 0,
		"no-return-assign": 0,
		"camelcase": 0,
		"quotes": [2, "double", { "avoidEscape": true }],
		"indent": ["error", "tab"],
		"react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
	}
}